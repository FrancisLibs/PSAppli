<form method="post" action="{{ path('user_delete', {'id': user.id}) }}" onsubmit="return confirm('Veux-tu vraiment supprimer cet utilisateur ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn btn-danger btn-sm">Supprimer</button>
</form>
